{
  "name": "yk-kit",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite --config ./build/base.config.ts",
    "doc": "vue-tsc --noEmit && vite build --config ./build/doc.config.ts && node script/copyDir.js",
    "lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
    "gen": "node ./script/genNewComp/gen.js",
    "f2elint-scan": "f2elint scan",
    "f2elint-fix": "f2elint fix",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "prismjs": "^1.25.0",
    "vue": "^3.2.23"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.11.12",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.3.7",
    "@vue/test-utils": "^2.4.1",
    "@vue/vue3-jest": "^29.2.6",
    "babel-core": "^6.26.3",
    "f2elint": "^2.2.1",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sass": "^1.57.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.0",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-dts": "^0.9.6",
    "vite-plugin-md": "^0.11.4",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-loader": "^15.11.1",
    "vue-router": "^4.0.12",
    "vue-tsc": "^1.0.24"
  },
  "husky": {
    "hooks": {
      "pre-commit": "f2elint commit-file-scan",
      "commit-msg": "f2elint commit-msg-scan"
    }
  },
  "engines": {
    "node": "16.20.2"
  }
}
